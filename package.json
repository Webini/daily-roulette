{
  "name": "dailyroulette",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/.bin/nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.test.ts\" src/index.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "db:migrate": "npm run typeorm migration:run",
    "lint": "npm run lint:eslint -- ./src",
    "lint:eslint": "NODE_ENV=production eslint --ext ts --ignore-path .gitignore",
    "lint:staged": "lint-staged",
    "test": "jest ./src/",
    "test:watch": "jest --watchAll ./src"
  },
  "lint-staged": {
    "*.js": "npm run-script lint:eslint"
  },
  "pre-commit": [
    "lint:staged"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@gitlab.webini.fr:22122/github/dailyroulette.git"
  },
  "author": "GASPARINI Nicolas",
  "license": "ISC",
  "dependencies": {
    "@slack/bolt": "^3.8.1",
    "@slack/web-api": "^6.5.1",
    "date-fns-tz": "^1.1.6",
    "debug": "^2.6.9",
    "dotenv": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.4.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.3",
    "lint-staged": "^12.1.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
